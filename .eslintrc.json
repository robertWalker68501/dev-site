{
  "extends": ["next/core-web-vitals", "standard", "plugin:tailwindcss/recommended", "prettier"],
  "rules": {
    "tailwindcss/no-custom-classname": "off",
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxEOF": 1
      }
    ],
    "object-curly-newline": 0
  }
}
